<script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>-->
<% let msgErros=[] %>
    <% let msgSuccess=[] %>
        <% success.forEach(function (msg) { %>
            <%msgSuccess.push(msg)%>
                <% }) %>
                    <% error.forEach(function (err) { %>
                        <%msgErros.push(err)%>
                            <% }) %>
                                <script>
                                    $(document).ready(function () {
                                        let error = "<%=msgErros%>"
                                        let success = "<%=msgSuccess%>"
                                        if (success !== '') {
                                            toastr.success(success, "Sucesso!")
                                            console.log(success)
                                        }
                                        error.split(',').forEach(function (err) {
                                            if (err !== '') {
                                                toastr.error(err, "Erro!")
                                                console.log(err)
                                            }
                                        })
                                        $('input').text().trim();
                                        $('[data-toggle="tooltip"]').tooltip()
                                        $('.nav-link').click(function () {
                                            $('.nav-link').removeClass('active');
                                            $(this).addClass('active');
                                        })
                                    })
                                </script>
                                <script>
                                    $('#imgPerfil')
                                        .mouseover(function () {
                                            $('#imgPerfil').addClass('animate__heartBeat');
                                        })
                                        .mouseout(function () {
                                            $('#imgPerfil').removeClass('animate__heartBeat');
                                        });
                                    $('#iconNotificacao')
                                        .mouseover(function () {
                                            $('#iconNotificacao').addClass('animate__heartBeat');
                                        })
                                        .mouseout(function () {
                                            $('#iconNotificacao').removeClass('animate__heartBeat');
                                        });
                                    $('#iconMsg')
                                        .mouseover(function () {
                                            $('#iconMsg').addClass('animate__heartBeat');
                                        })
                                        .mouseout(function () {
                                            $('#iconMsg').removeClass('animate__heartBeat');
                                        });
                                </script>

